{
  "name": "dayz-rstrt",
  "version": "0.1.0",
  "description": "DayZ Server Manager - Cross-platform desktop application",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && set ELECTRON_BUILDER_CACHE=%TEMP%\\electron-builder-cache&& electron-builder --dir",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "keywords": [
    "dayz",
    "server-manager",
    "rcon",
    "electron",
    "react"
  ],
  "author": "mazix <mazix@bk.ru>",
  "license": "GPL-2.0",
  "dependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "antd": "^5.12.0",
    "battle-node-v2": "^2.0.0",
    "body-parser": "^2.2.2",
    "cors": "^2.8.5",
    "electron-store": "^8.1.0",
    "express": "^5.2.1",
    "gamedig": "^4.0.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tree-kill": "^1.2.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^39.2.7",
    "electron-builder": "^24.9.1",
    "typescript": "^5.3.3",
    "vite": "^7.3.1",
    "vite-plugin-electron": "^0.15.4",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "build": {
    "appId": "com.mazix.dayzrstrt",
    "productName": "DayZ-RSTRT",
    "directories": {
      "output": "dist_electron_build"
    },
    "win": {
      "target": "dir",
      "forceCodeSigning": false
    }
  }
}
